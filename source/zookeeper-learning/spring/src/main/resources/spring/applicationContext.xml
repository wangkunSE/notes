<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="car" class="com.soul.spring.demo.domain.Car" init-method="myInit" destroy-method="myDestroy"
           p:brand="宝马X5" p:speed="100" />

    <bean id="myBeanPostProcessor" class="com.soul.spring.demo.processor.MyBeanPostProcessor"/>

    <bean id="myInstantiationAwareBeanPostProcessor" class="com.soul.spring.demo.processor.MyInstantiationAwareBeanPostProcessor"/>
</beans>